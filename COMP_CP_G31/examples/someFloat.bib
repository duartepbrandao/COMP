@Article{Grau:1962:FNR,
  author =       "A. A. Grau",
  title =        "On a floating-point number representation for use with
                 algorithmic languages",
  journal =      j-CACM,
  volume =       "5",
  number =       "3",
  pages =        "160--161",
  month =        mar,
