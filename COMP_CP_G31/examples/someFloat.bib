  title =        "{Triplex} Arithmetic for {Fortran}",
  type =         "MRC Technical Summary",
  number =       "1901",
  institution =  inst-MRC-WISCONSIN,
  address =      inst-MRC-WISCONSIN:adr,
  pages =        "38",
  month =        dec,
