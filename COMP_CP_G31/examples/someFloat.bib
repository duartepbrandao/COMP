@InProceedings{Stephenson:1975:CSP,
  author =       "Charles Stephenson",
  title =        "Case Study of the Pipelined Arithmetic Unit for the
                 {TI Advanced Scientific Computer}",
  crossref =     "IEEE:1975:SCA",
