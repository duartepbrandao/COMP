@Article{Wright:1990:NCU,
  author =       "F. Wright",
  title =        "The `effective number of codons' used in a gene",
  journal =      j-GENE,
  volume =       "87",
  number =       "1",
  pages =        "23--29",
  day =          "1",
  month =        mar,
