@MastersThesis{Sullivan:1991:VPI,
  author =       "Steve Sullivan",
  title =        "Vector and parallel implementations of the wavelet
                 transform",
  type =         "Thesis (M.S.)",
  number =       "CSRD 1083; UILU-ENG-91-8034",
  school =       inst-UIUC-CSRD,
  address =      inst-UIUC-CSRD:adr,
