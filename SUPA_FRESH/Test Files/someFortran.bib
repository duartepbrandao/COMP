  pages =        "35--39",
  year =         "1990",
  CODEN =        "SPLTDC",
  DOI =          "http://dx.doi.org/10.1016/0167-7152(90)90092-L",
  ISSN =         "0167-7152 (print), 1879-2103 (electronic)",
