@Article{Wichmann:1989:SPI,
  author =       "B. A. Wichmann",
  title =        "Scientific processing in {ISO-Pascal}: a proposal to
                 get the benefits of mixed precision floating-point",
  journal =      j-SIGPLAN,
  volume =       "24",
  number =       "6",
  pages =        "20--22",
  month =        jun,
  year =         "1989",
  CODEN =        "SINODQ",
  ISSN =         "0362-1340 (print), 1523-2867 (print), 1558-1160
