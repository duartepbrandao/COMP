//START=REFERENCIA
void Start() : {Token t1;}
{
	[Order()]  (Reference())+  <EOF>//"\n"//mudar p/ <EOF> + tarde
}

void Order() : {Token t1;}
{
	try {
	<ORDER_SIGN> t1=<ORDER_ENTRY> { order_token = t1.image.toLowerCase(); }
	}
	catch (ParseException e) {
	 System.out.println("Warning:Order TAG badly defined. use ## followed by an entry name;");
	}
}

